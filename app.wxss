/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.six-bar,.four-bar{
  display: flex;
  flex-flow: column;
  margin-top: 20rpx;
  padding: 0 10rpx 20rpx 10rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #EDEEF0;
}
.bar-title{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 17px;
  font-weight: bold;
}
.bar-title-text{
  
}
.bar-title-more,.bar-title-change{
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #ccc;
  box-shadow: 0 0 2px #ccc;
  font-size: 14px;
  font-weight: normal;
  border-radius: 24rpx;
  padding: 4rpx 20rpx;
  box-sizing: border-box;
  background: #ccc;
  color: #fff;
}
.bar-title-change>image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.six-bar-box,.four-bar-box{
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  margin-top: 20rpx;
}
.six-bar-box-item{
  flex: 0 1 30%;
  display: flex;
  flex-flow: column;
  justify-content: space-around;
  height: 330rpx;
  width: 230rpx;
  margin-bottom: 10rpx;
}
.four-bar-box-item{
  flex: 0 1 48%;
  display: flex;
  flex-flow: column;
  justify-content: space-around;
  height: 330rpx;
  width: 230rpx;
  margin-bottom: 10rpx;
}
.six-bar-box-item-image{
  height: 230rpx;
  width: 230rpx;
  border-radius: 20rpx;
}
.four-bar-box-item-image{
  height: 230rpx;
  width: 100%;
  border-radius: 20rpx;
}
.six-bar-box-item-name,.four-bar-box-item-name{
  font-size: 16px;
}
.six-bar-box-item-singer,.four-bar-box-item-singer{
  font-size: 11px;
}
.six-bar-box-item-text,.four-bar-box-item-text{
  font-size: 12px;
}
.ellipsisText{
  /* display: flex;
  align-items: center; */
  /* justify-content: center; */
  /* display: flex;
  align-items: center; */
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  word-wrap: break-word;
  word-break: break-all;
}
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1857271_chw3ir1a6j.eot?t=1591068174322'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1857271_chw3ir1a6j.eot?t=1591068174322#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1857271_chw3ir1a6j.woff?t=1591068174322') format('woff'),
  url('//at.alicdn.com/t/font_1857271_chw3ir1a6j.ttf?t=1591068174322') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1857271_chw3ir1a6j.svg?t=1591068174322#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

